#[import(cc = "C")] fn ignis_set_error_count(i32) -> ();

#[export] fn test_main(_settings: &Settings, _userData: &mut [i32]) -> () {

    let mut err = test_bbox();
    err += test_matrix();
    err += test_intersection();
    err += test_interval();
    err += test_microfacet();
    err += test_cdf();
    err += test_warp() ;
    err += test_reduction(NoGPU);
    
    ignis_set_error_count(err);
}
